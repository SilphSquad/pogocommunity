    <!-- Site theme -->
    <link href="{{ asset('css/pogocomm-theme.css') }}?v=20190916" rel="stylesheet">

    <meta property="og:url" content="{{ url().current() }}" />
    <meta property="og:title" content="{{ PageTitle }}" />

</head>

<body>

<header>
    {% include 'theme/pogo/nav-top.twig' %}
